DROP TABLE transactions;


CREATE TABLE transactions (
    transaction_id INT AUTO_INCREMENT PRIMARY KEY,
    user_id VARCHAR(50) NOT NULL,
    amount REAL NOT NULL,
    transaction_type VARCHAR(50)
);